<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  
  
  <servlet>
    <servlet-name>loginService</servlet-name>
    <servlet-class>com.google.gwt.sample.itProjekt.server.LoginServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>loginService</servlet-name>
    <url-pattern>/itprojekt_ss18_gr_10/login</url-pattern>
    <url-pattern>/itprojekt_ss18_gr_10_report/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
		<servlet-name>reportGeneratorServlet</servlet-name>
		<servlet-class>com.google.gwt.sample.itProjekt.server.ReportGeneratorImpl</servlet-class>
  </servlet>

  <servlet-mapping>
		<servlet-name>reportGeneratorServlet</servlet-name>
		<url-pattern>/itprojekt_ss18_gr_10/reportgenerator</url-pattern>
		<url-pattern>/itprojekt_ss18_gr_10_report/reportgenerator</url-pattern>
  </servlet-mapping>
  
  <servlet>
		<servlet-name>editorAdministrationServlet</servlet-name>
		<servlet-class>com.google.gwt.sample.itProjekt.server.EditorAdministrationImpl</servlet-class>
  </servlet>

  <servlet-mapping>
		<servlet-name>editorAdministrationServlet</servlet-name>
		<url-pattern>/itprojekt_ss18_gr_10/editoradministration</url-pattern>
		<url-pattern>/itprojekt_ss18_gr_10_report/editoradministration</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
    <welcome-file>ITProjekt_SS18_Gr_10_Report.html</welcome-file>
 	<welcome-file>ITProjekt_SS18_Gr_10.html</welcome-file>
   </welcome-file-list>

</web-app>